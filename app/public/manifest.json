{
  "name": "Shorty Browsy",
  "description": "The easiest way of browsing with shortcuts",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Shorty Browsy",
    "default_popup": "index.html"
  },
  "icons": {
    "48": "icon32.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "OpenUp",
      "global": false
    }
  }
}