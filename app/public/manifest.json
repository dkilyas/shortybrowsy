{
  "name": "Shorty Browsy",
  "description": "The easiest way of browsing with shortcuts",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Shorty Browsy"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ],
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Toggle feature foo"
    }
  }
}